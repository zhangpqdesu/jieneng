<view class="out"><image src="{{list.imgUrl}}" mode="aspectFit"></image><view class="box1"><view class="slide"></view><view class="type"><block wx:for="{{typeArr}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['clickType',[index]]]]]}}" class="{{['item',typeIndex==index?'active':'']}}" style="font-size:20rpx;" bindtap="__e">{{item.title}}</view></block></view><block wx:if="{{typeIndex==0}}"><view class="box2"><view class="content">主轴转速:<input data-event-opts="{{[['input',[['__set_model',['$0','rpm','$event',[]],['list']]]]]}}" class="input" value="{{list.rpm}}" bindinput="__e"/>r/s</view><view class="content">主轴流量:<input data-event-opts="{{[['input',[['__set_model',['$0','flow','$event',[]],['list']]]]]}}" class="input" value="{{list.flow}}" bindinput="__e"/></view><view class="content">滞止压力:<input data-event-opts="{{[['input',[['__set_model',['$0','pressure','$event',[]],['list']]]]]}}" class="input" value="{{list.pressure}}" bindinput="__e"/>V</view><view class="content">修正系数:<input data-event-opts="{{[['input',[['__set_model',['$0','correction','$event',[]],['list']]]]]}}" class="input" value="{{list.correction}}" bindinput="__e"/></view><view class="content">额定功率:<input data-event-opts="{{[['input',[['__set_model',['$0','power','$event',[]],['list']]]]]}}" class="input" value="{{list.power}}" bindinput="__e"/>kW</view><view class="content">效率:<input data-event-opts="{{[['input',[['__set_model',['$0','efficiency','$event',[]],['list']]]]]}}" class="input" value="{{list.efficiency}}" bindinput="__e"/></view><view class="content">型号:<input data-event-opts="{{[['input',[['__set_model',['$0','model','$event',[]],['list']]]]]}}" class="input" value="{{list.model}}" bindinput="__e"/></view></view></block><block wx:else><block wx:if="{{typeIndex==1}}"><view class="box2"><view class="content">转速:<input data-event-opts="{{[['input',[['__set_model',['$0','rotated_speed','$event',[]],['list']]]]]}}" class="input" value="{{list.rotated_speed}}" bindinput="__e"/>r/min</view><view class="content">效率（%）:<input data-event-opts="{{[['input',[['__set_model',['$0','efficiency','$event',[]],['list']]]]]}}" class="input" value="{{list.efficiency}}" bindinput="__e"/></view><view class="content">型号:<input data-event-opts="{{[['input',[['__set_model',['$0','model','$event',[]],['list']]]]]}}" class="input" value="{{list.model}}" bindinput="__e"/></view><view class="content">额定功率:<input data-event-opts="{{[['input',[['__set_model',['$0','power','$event',[]],['list']]]]]}}" class="input" value="{{list.power}}" bindinput="__e"/>kW</view><view class="content">出厂时间：<input data-event-opts="{{[['input',[['__set_model',['$0','run_time','$event',[]],['list']]]]]}}" class="input" value="{{list.run_time}}" bindinput="__e"/></view></view></block><block wx:else><view class="box2"><view class="content">主轴转速:<input data-event-opts="{{[['input',[['__set_model',['$0','rotated_speed','$event',[]],['list']]]]]}}" class="input" value="{{list.rotated_speed}}" bindinput="__e"/>r/min</view><view class="content">主轴流量:<input data-event-opts="{{[['input',[['__set_model',['$0','flowRate','$event',[]],['list']]]]]}}" class="input" value="{{list.flowRate}}" bindinput="__e"/>m³/h</view><view class="content">扬程:<input data-event-opts="{{[['input',[['__set_model',['$0','head','$event',[]],['list']]]]]}}" class="input" value="{{list.head}}" bindinput="__e"/>m</view><view class="content">型号:<input data-event-opts="{{[['input',[['__set_model',['$0','model','$event',[]],['list']]]]]}}" class="input" value="{{list.model}}" bindinput="__e"/></view></view></block></block><view class="line"></view><image class="import" src="/static/photo/load.png" mode="aspectFit" data-event-opts="{{[['tap',[['clickImport',['$event']]]]]}}" bindtap="__e"></image><view value="{{energy_consumption}}" data-event-opts="{{[['input',[['__set_model',['','energy_consumption','$event',[]]]]]]}}" bindinput="__e" class="result"><view>能效等级：</view><view style="font-weight:bold;">{{energy_consumption}}</view></view><view value="{{list.is_backward}}" data-event-opts="{{[['input',[['__set_model',['$0','is_backward','$event',[]],['list']]]]]}}" bindinput="__e" class="result"><view>是否淘汰：</view><view style="font-weight:bold;">{{list.is_backward}}</view></view><view value="{{list.batch}}" data-event-opts="{{[['input',[['__set_model',['$0','batch','$event',[]],['list']]]]]}}" bindinput="__e" class="result"><view>来源：</view><view style="font-weight:bold;">{{list.batch}}</view></view><view style="font-weight:bold;">备注:<input data-event-opts="{{[['input',[['__set_model',['$0','extraInfo','$event',[]],['list']]]]]}}" class="input" value="{{list.extraInfo}}" bindinput="__e"/></view><view class="flip"><button class="button" hover-class="button2">上一页</button><button class="button" hover-class="button2">下一页</button></view><button data-event-opts="{{[['tap',[['save_records',['$event']]]]]}}" class="send-button" bindtap="__e">发送数据到后端</button><button class="return" hover-class="return2" data-event-opts="{{[['tap',[['clickReturn',['$event']]]]]}}" bindtap="__e">返回首页</button></view></view>