<view class="out"><image src="{{list.imgUrl}}" mode="aspectFit"></image><view class="box1"><view class="slide"></view><view class="type"><block wx:for="{{typeArr}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['clickType',[index]]]]]}}" class="{{['item',typeIndex==index?'active':'']}}" style="font-size:20rpx;" bindtap="__e">{{item.title}}</view></block></view><block wx:if="{{typeIndex==0}}"><view class="box2"><view class="content">主轴转速:<input data-event-opts="{{[['input',[['__set_model',['$0','rpm','$event',[]],['list']]]]]}}" class="input" value="{{list.rpm}}" bindinput="__e"/>r/s</view><view class="content">主轴流量:<input data-event-opts="{{[['input',[['__set_model',['$0','flow','$event',[]],['list']]]]]}}" class="input" value="{{list.flow}}" bindinput="__e"/></view><view class="content">滞止压力:<input data-event-opts="{{[['input',[['__set_model',['$0','press','$event',[]],['list']]]]]}}" class="input" value="{{list.press}}" bindinput="__e"/>V</view><view class="content">修正系数:<input data-event-opts="{{[['input',[['__set_model',['$0','correction','$event',[]],['list']]]]]}}" class="input" value="{{list.correction}}" bindinput="__e"/></view><view class="content">额定功率:<input data-event-opts="{{[['input',[['__set_model',['$0','feng_power','$event',[]],['list']]]]]}}" class="input" value="{{list.feng_power}}" bindinput="__e"/>kW</view><view class="content">效率:<input data-event-opts="{{[['input',[['__set_model',['$0','feng_efficiency','$event',[]],['list']]]]]}}" class="input" value="{{list.feng_efficiency}}" bindinput="__e"/></view></view></block><block wx:else><block wx:if="{{typeIndex==1}}"><view class="box2"><view class="content">转速:<input data-event-opts="{{[['input',[['__set_model',['$0','rotated_speed','$event',[]],['list']]]]]}}" class="input" value="{{list.rotated_speed}}" bindinput="__e"/>r/min</view><view class="content">效率（%）:<input data-event-opts="{{[['input',[['__set_model',['$0','efficiency','$event',[]],['list']]]]]}}" class="input" value="{{list.efficiency}}" bindinput="__e"/></view><view class="content">型号:<input data-event-opts="{{[['input',[['__set_model',['$0','motor_type','$event',[]],['list']]]]]}}" class="input" value="{{list.motor_type}}" bindinput="__e"/></view><view class="content">额定功率:<input data-event-opts="{{[['input',[['__set_model',['$0','power','$event',[]],['list']]]]]}}" class="input" value="{{list.power}}" bindinput="__e"/>kW</view></view></block><block wx:else><view class="box2"><view class="content">主轴转速:<input data-event-opts="{{[['input',[['__set_model',['$0','rpm','$event',[]],['list']]]]]}}" class="input" value="{{list.rpm}}" bindinput="__e"/>r/s</view><view class="content">主轴流量:<input data-event-opts="{{[['input',[['__set_model',['$0','flow','$event',[]],['list']]]]]}}" class="input" value="{{list.flow}}" bindinput="__e"/></view><view class="content">滞止压力:<input data-event-opts="{{[['input',[['__set_model',['$0','press','$event',[]],['list']]]]]}}" class="input" value="{{list.press}}" bindinput="__e"/>V</view><view class="content">修正系数:<input data-event-opts="{{[['input',[['__set_model',['$0','correction','$event',[]],['list']]]]]}}" class="input" value="{{list.correction}}" bindinput="__e"/></view></view></block></block><view class="line"></view><image class="import" src="/static/photo/导入.png" mode="aspectFit" data-event-opts="{{[['tap',[['clickImport',['$event']]]]]}}" bindtap="__e"></image><view value="{{list.batch}}" data-event-opts="{{[['input',[['__set_model',['$0','batch','$event',[]],['list']]]]]}}" bindinput="__e" class="result"><view>能效等级：</view><view style="font-weight:bold;">{{list.grade}}</view></view><view value="{{list.batch}}" data-event-opts="{{[['input',[['__set_model',['$0','batch','$event',[]],['list']]]]]}}" bindinput="__e" class="result"><view>是否淘汰：</view><view style="font-weight:bold;">{{list.is_backward}}</view></view><view value="{{list.batch}}" data-event-opts="{{[['input',[['__set_model',['$0','batch','$event',[]],['list']]]]]}}" bindinput="__e" class="result"><view>来源：</view><view style="font-weight:bold;">{{list.batch}}</view></view><view class="flip"><button class="button" hover-class="button2">上一页</button><button class="button" hover-class="button2">下一页</button></view><button class="return" hover-class="return2" data-event-opts="{{[['tap',[['clickReturn',['$event']]]]]}}" bindtap="__e">返回首页</button></view></view>